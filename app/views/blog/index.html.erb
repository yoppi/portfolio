<h2>Blog:<a href="<%= @blog.channel.link %>"><%= @blog.channel.title %></a></h2>
<div id="blog-items">
<% @blog.items.each do |item| %>
<div class="blog-item">
  <div class="blog-title"><%= item.title %></div>
  <div class="blog-date"><%= item.date %></div>
  <div class="blog-description"><%= item.description %></div>
</div>
<% end %>
</div>
